{
  "name": "electron_demo",
  "version": "1.0.0",
  "description": "https://github.com/vuejs/create-vue\r https://github.com/alex8088/electron-vite",
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "concurrently \"npm run serve\" \"wait-on http://localhost:8080 && electron .\"",
    "vbuild": "vue-cli-service build",
    "build": "npm run vbuild && electron-builder build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "src/main/index.js",
  "build": {
    "files": [
      "dist/**/*",
      "src/main/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "src/resources/models",
        "to": "resources/models"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xcyxiner/electron_demo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xcyxiner/electron_demo/issues"
  },
  "homepage": "https://github.com/xcyxiner/electron_demo#readme",
  "dependencies": {
    "three": "^0.173.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vue/cli-service": "^5.0.8",
    "concurrently": "^9.1.2",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "electron-reloader": "^1.2.3",
    "nodemon": "^3.1.9",
    "worker-loader": "^3.0.8"
  }
}
